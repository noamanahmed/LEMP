#!/bin/bash

DIR=$(dirname "${BASH_SOURCE[0]}") 
DIR=$(realpath "${DIR}") 

template_path="$(cd $DIR/../ && pwd)/templates"
source $DIR/../includes/helpers.sh
if [ "$EUID" -ne 0 ]
  then echo "Please run as root"
  exit
fi


sites='$(members web)'

for user in $sites    
do
    if [ "$user" == "web" ]
    then
        continue
    fi
    bash site-details -u $user
done